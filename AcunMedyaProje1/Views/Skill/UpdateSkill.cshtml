@model AcunMedyaProje1.Models.TBL_Skill
@{
    ViewBag.Title = "UpdateSkill";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yetenek Güncelleme Sayfası</h2>

<form method="post">

    @Html.HiddenFor(x => x.SkillID)
    <label>Yetenek Adı : </label>
    @Html.TextBoxFor(x => x.SkillName)
    <br />
    <br />
    <label>Yetkinlik Derecesi : </label>
    @Html.TextBoxFor(x => x.Derece)
    <br />
    <br />
    <label>Açıklama : </label>
    @Html.TextBoxFor(x => x.Description)
    <br />
    <br />
    <button class="btn btn-primary">Güncelle</button>

</form>