
@{
    ViewBag.Title = "CreateSkill";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yetenek Oluşturma Sayfası</h2>

<form method="post">

    <label>Yetenek Adı</label>
    <input type="text" class="form-control" style="width : 500px;" name="SkillName" />
    <br />
    <label>Derece</label>
    <input type="text" class="form-control" style="width : 500px;" name="Derece" />
    <br />
    <label>Açıklama</label>
    <input type="text" class="form-control" style="width : 500px;" name="Description" />
    <br />
    <button class="btn btn-success">Ekle</button>

</form>

