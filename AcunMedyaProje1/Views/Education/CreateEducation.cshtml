
@{
    ViewBag.Title = "CreateEducation";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Eğitim Oluşturma Sayfası</h2>

<form method="post">

    <label>Üniversite Adı</label>
    <input type="text" class="form-control" style="width : 500px;" name="Name" />
    <br />
    <label>Başlangıç Yılı</label>
    <input type="text" class="form-control" style="width : 500px;" name="StartedYear" />
    <br />
    <label>Bitiş Yılı</label>
    <input type="text" class="form-control" style="width : 500px;" name="EndYear" />
    <br />
    <label>Açıklama</label>
    <input type="text" class="form-control" style="width : 500px;" name="Description" />
    <br />
    <label>Bölüm</label>
    <input type="text" class="form-control" style="width : 500px;" name="Section" />
    <br />



    <button class="btn btn-success">Ekle</button>

</form>





