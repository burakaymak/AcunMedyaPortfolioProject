@model AcunMedyaProje1.Models.TBL_Services
@{
    ViewBag.Title = "UpdateService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Hizmet Güncelleme Sayfası</h2>


<form method="post">
    @Html.HiddenFor(x => x.ServiceID)
    <label>Servis Adı : </label>
    @Html.TextBoxFor(x => x.Title)
    <br />
    <br />
    <label>Açıklama : </label>
    @Html.TextBoxFor(x => x.Description)
    <br />
    <br />
    <label>Açıklama 2: </label>
    @Html.TextBoxFor(x => x.Descripiton2)
    <br />
    <br />
    <label>İkon Url: </label>
    @Html.TextBoxFor(x => x.IconURL)
    <br />
    <br />
    <button class="btn btn-primary">Güncelle</button>



</form>

