
@{
    ViewBag.Title = "CreateJob";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Deneyim Ekleme Sayfası</h2>

<form method="post">

    <label>Pozisyon</label>
    <input type="text" class="form-control" style="width : 500px;" name="Title" />
    <br />
    <label>Başlangıç Tarihi</label>
    <input type="text" class="form-control" style="width : 500px;" name="StartDate" />
    <br />
    <label>Bitiş Tarihi</label>
    <input type="text" class="form-control" style="width : 500px;" name="EndDate" />
    <br />
    <label>Şirket Adı</label>
    <input type="text" class="form-control" style="width : 500px;" name="CompanyName" />
    <br />
    <label>Açıklama</label>
    <input type="text" class="form-control" style="width : 500px;" name="Description" />
    <br />
    <button class="btn btn-success">Ekle</button>

</form>

