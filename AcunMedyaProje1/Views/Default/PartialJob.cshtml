@model List<AcunMedyaProje1.Models.TBL_Job>

<div class="col-lg-6" data-aos="fade-up" data-aos-delay="200">
    <h3 class="resume-title">Work Experiences</h3>

    @foreach (var item in Model)
    {
        <div class="resume-item">
            <h4>@item.Title</h4>
            <h5>
                @(item.StartDate.HasValue ? item.StartDate.Value.ToString("dd/MM/yyyy") : "") 
                -
                @(item.EndDate.HasValue ? item.EndDate.Value.ToString("dd/MM/yyyy") : "Present")
            </h5>
            <p><em>@item.CompanyName</em></p>
            <ul>
                <li>@item.Description</li>
            </ul>
        </div><!-- End Resume Item -->
    }
</div>
