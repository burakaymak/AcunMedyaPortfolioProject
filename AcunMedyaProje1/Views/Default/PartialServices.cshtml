@model List<AcunMedyaProje1.Models.TBL_Services>

<section id="services" class="services section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Services</h2>
        <p>A range of practical, user-focused services shaped by real project experience and technical expertise.</p>
    </div><!-- End Section Title -->

    <div class="container">

        <div class="row gy-4">


            @foreach (var item in Model)
            {
                <div class="col-xl-3 col-md-6 d-flex" data-aos="fade-up" data-aos-delay="100">
                    <div class="service-item position-relative">
                        <div class="icon"><i class="@item.IconURL"></i></div>
                        <h4><a href="" class="stretched-link">@item.Title</a></h4>
                        <p>@item.Descripiton2</p>
                    </div>
                </div>
            }

            <!-- End Service Item -->


        </div>

    </div>

</section>
