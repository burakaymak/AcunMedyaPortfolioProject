<body class="index-page">

    <header id="header" class="header d-flex align-items-center sticky-top">
        <div class="container-fluid container-xl position-relative d-flex align-items-center justify-content-between">

            <a href="index.html" class="logo d-flex align-items-center">
                <!-- Uncomment the line below if you also wish to use an image logo -->
                <!-- <img src="assets/img/logo.png" alt=""> -->
                <h1 class="sitename">burakkaymak</h1>
            </a>

            <nav id="navmenu" class="navmenu">
                <ul>
                    <li><a href="#hero" class="active">Home</a></li>
                    <li><a href="#about">About</a></li>
                    <li><a href="#team">Skills</a></li>
                    <li><a href="#services">Services</a></li>
                    <li><a href="#contact">Contact</a></li>
                </ul>
                <i class="mobile-nav-toggle d-xl-none bi bi-list"></i>
            </nav>

        </div>
    </header>

    <main class="main">

        <!-- Hero Section -->
        @Html.Action("PartialSlider", "Default")
        <!-- /Hero Section -->
        <!-- About Section -->
        @Html.Action("PartialAbout", "Default")
        <!-- /About Section -->
        <!-- Skills Section -->
        @Html.Action("PartialSkills","Default")
        <!-- /Skills Section -->
        <!-- Resume Section -->
        <section id="resume" class="resume section light-background">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Resume</h2>
                <p>An overview of my ongoing academic and professional journey.</p>
            </div><!-- End Section Title -->

            <div class="container">

                <div class="row">

                    @Html.Action("PartialEducation", "Default")
                    @Html.Action("PartialJob", "Default")


                    

                </div>

            </div>

        </section><!-- /Resume Section -->


        <!-- Services Section -->
        @Html.Action("PartialServices", "Default")
        <!-- /Services Section -->
        <!-- Portfolio Section -->
        <!-- /Portfolio Section -->
        <!-- Testimonials Section -->
        @Html.Action("PartialTestimonial", "Default")
        <!-- /Testimonials Section -->
        <!-- Contact Section -->
        <section id="contact" class="contact section">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Contact</h2>
                <p>Interested in working together or want to get in touch? I'm just a message away.</p>
            </div><!-- End Section Title -->

            <div class="container" data-aos="fade" data-aos-delay="100">

                <div class="row gy-4">

                    @Html.Action("PartialContact", "Default")



                    @Html.Action("PartialMessage", "Default")<!-- End Contact Form -->

                </div>

            </div>

        </section><!-- /Contact Section -->

    </main>

